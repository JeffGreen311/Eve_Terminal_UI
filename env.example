# Copy to .env (do NOT commit .env)
# Required for Replicate-based model calls
REPLICATE_API_TOKEN=<REPLICATE_API_TOKEN>

# Optional (only if you enable ElevenLabs integration)
ELEVENLABS_API_KEY=<ELEVENLABS_API_KEY>

# Optional: Ollama local inference
OLLAMA_API_URL=http://localhost:11434/api/generate
OLLAMA_MODEL=eve-consciousness

# Optional: Where Eve writes local creative output (defaults to ~/.eve/creative_outlets)
EVE_OUTPUT_DIR=

# Optional: Where autonomous dream images are stored (defaults to ~/.eve/autonomous_dreaming/generated_content/dream_images)
EVE_AUTONOMOUS_DREAM_DIR=

# Optional: Premium local model directory (if you use it)
EVE_PREMIUM_QWEN_DIR=<EVE_PREMIUM_QWEN_DIR>

# Optional: If your app expects a local API key for internal auth
EVE_API_KEY=<EVE_API_KEY>

# Safety controls (recommended defaults)
EVE_ENABLE_AUTO_DEP_CHECK=0
EVE_AUTO_INSTALL_DEPS=0
